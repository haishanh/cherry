{
  "name": "cherry",
  "version": "0.0.8",
  "private": true,
  "license": "UNLICENSED",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-svelte"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "dev": "vite dev",
    "test": "vitest",
    "do": "NODE_OPTIONS='--import tsx' node",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "sync": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "bundle:cli": "esbuild src/cli/index.ts --bundle --minify --platform=node --target=node16 --outfile=cherry",
    "cherry": "tsx src/cli/index.ts",
    "lint": "prettier --check src scripts && eslint --cache src",
    "lint:fix": "eslint --cache --fix src",
    "prettier:fix": "prettier --write src scripts",
    "fix": "pnpm lint:fix && pnpm prettier:fix"
  },
  "devDependencies": {
    "@browser-logos/chrome": "2.0.0",
    "@browser-logos/firefox": "3.0.10",
    "@browser-logos/safari": "2.1.0",
    "@eslint/js": "9.13.0",
    "@fontsource/inter": "5.1.0",
    "@sveltejs/adapter-node": "5.2.9",
    "@sveltejs/kit": "2.7.3",
    "@sveltejs/package": "2.3.7",
    "@sveltejs/vite-plugin-svelte": "4.0.0",
    "@types/better-sqlite3": "7.6.11",
    "@types/debug": "4.1.12",
    "@types/estree": "1.0.6",
    "@types/tar": "6.1.13",
    "autoprefixer": "10.4.20",
    "axios": "1.7.7",
    "domhandler": "5.0.3",
    "esbuild": "0.24.0",
    "esbuild-register": "3.6.0",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-svelte": "^2.45.1",
    "globals": "15.11.0",
    "ky": "1.7.2",
    "lucide-svelte": "0.453.0",
    "nanoid": "5.0.7",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prettier-plugin-svelte": "3.2.7",
    "sade": "1.8.1",
    "sass": "1.80.4",
    "svelte": "5.1.3",
    "svelte-check": "4.0.5",
    "svelte-file-dropzone": "2.0.9",
    "tiny-invariant": "1.3.3",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.8.0",
    "tsx": "4.19.1",
    "typescript": "5.6.3",
    "typescript-eslint": "8.11.0",
    "undici": "6.20.1",
    "vite": "5.4.10",
    "vitest": "2.1.3"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "3.679.0",
    "@aws-sdk/lib-storage": "3.679.0",
    "@node-rs/argon2": "1.8.3",
    "@node-rs/crc32": "1.10.3",
    "better-sqlite3": "11.5.0",
    "csv-stringify": "6.5.1",
    "date-fns": "4.1.0",
    "debug": "4.3.7",
    "dotenv": "16.4.5",
    "file-type": "19.6.0",
    "htmlparser2": "9.1.0",
    "tar": "7.4.3",
    "zod": "3.23.8"
  }
}
